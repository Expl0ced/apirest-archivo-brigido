{
    "version": 2,
    "builds": [
        {
            "src": "./app.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/",
            "methods": [
                "GET",
                "POST",
                "PUT",
                "DELETE",
                "UPDATE",
                "PATCH",
                "OPTIONS",
                "CONNECT",
                "TRACE"
            ],
            "headers": {    
                "Access-Control-Allow-Origin": "*",
                "Access-Control-Allow-Headers": "Content-Type"
            }
        }
    ]
}   